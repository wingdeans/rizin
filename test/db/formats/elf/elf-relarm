NAME=ELF: arm64 relocs 2
FILE=bins/elf/graphascii.c-gcc-arm64-O0.o
CMDS=<<EOF
ir~?
# AARCH64_ADR_PREL_PG_HI21
s 0x8000444
pf bbbb
pi 1
# AARCH64_CALL26
s 0x8000460
pf bbbb
pi 1
# AARCH64_LD64_GOT_LO12
s 0x800046c
pf bbbb
pi 1
# AARCH64_CALL26
s 0x80003d8
pf bbbb
pi 1
EOF
EXPECT=<<EOF
315
0x08000444 = 0x00
0x08000445 = 0x00
0x08000446 = 0x00
0x08000447 = 0xb0
adrp x0, 0x8001000
0x08000460 = 0x28
0x08000461 = 0x4a
0x08000462 = 0x00
0x08000463 = 0x54
b.hi stackCursor
0x0800046c = 0x20
0x0800046d = 0x58
0x0800046e = 0x60
0x0800046f = 0xb8
